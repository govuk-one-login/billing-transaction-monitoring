{
  "name": "di-billing-transaction-monitoring",
  "description": "Monitors user identity verification events for billing purposes",
  "version": "0.0.1",
  "type": "module",
  "devDependencies": {
    "@aws-sdk/client-athena": "^3.188.0",
    "@aws-sdk/client-cloudformation": "^3.188.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.188.0",
    "@aws-sdk/client-s3": "^3.188.0",
    "@aws-sdk/client-sns": "^3.188.0",
    "@aws-sdk/client-sqs": "^3.188.0",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@types/aws-lambda": "^8.10.101",
    "@types/express": "^4.17.1",
    "@types/jest": "^29.0.1",
    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.38.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk-client-mock": "^2.0.0",
    "babel-jest": "^29.0.3",
    "csvtojson": "^2.0.10",
    "delay": "^5.0.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-allure": "^0.1.3",
    "jest-jasmine2": "^29.2.2",
    "jest-junit": "^14.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "yaml": "^2.1.3"
  },
  "scripts": {
    "husky:install": "husky install",
    "test": "jest",
    "clean:allureFolders": "rm -R -f allure-reports/* && rm -R -f allure-results/*",
    "beforeIntTest": "npm run clean:allureFolders",
    "test:integration": "jest --config jest.integration.config.ts",
    "test:integration:local": "jest --setupFiles=./integration_tests/setEnvVars.ts --testPathIgnorePatterns athena-query-tests s3-vat-tests --config jest.integration.config.ts",
    "build": "webpack",
    "lint": "eslint . --fix",
    "build:template": "node ./src/tools/yaml-import.js",
    "checkov": "checkov --quiet -f template.yaml --framework cloudformation --external-checks-git git@github.com:alphagov/di-devplatform-checkov-hook.git//src/gds_digitalidentity_checkovhook/custom_policies",
    "sam:build": "npm run build && npm run build:template && npm run checkov && sam build",
    "sam:deploy": "sam deploy --stack-name di-btm-${ENV_NAME} --parameter-overrides Environment=${ENV_NAME} --s3-prefix ${ENV_NAME}",
    "sam:teardown": "ts-node --esm src/tools/teardown-cf-stack.ts"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,json,ts,yaml,yml}": "prettier --write"
  }
}
